{
  "name": "hanzi-writer",
  "version": "0.2.0",
  "author": "David Chanin <chanindav@gmail.com> (http://chanind.github.io/hanzi-writer/)",
  "description": "Hanzi Writer is a free and open-source javascript library for both animating simplified Chinese characters and quizzing users on character stroke order.",
  "repository": "chanind/hanzi-writer",
  "main": "dist/hanzi-writer-lib.js",
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "*",
    "babel-jest": "*",
    "babel-loader": "^6.0.0",
    "babel-polyfill": "*",
    "babel-preset-es2015-loose": "*",
    "babel-preset-stage-2": "^6.0.0",
    "eslint-config-airbnb": "*",
    "eslint-loader": "*",
    "eslint-plugin-react": "*",
    "grunt": "*",
    "grunt-contrib-clean": "*",
    "grunt-contrib-copy": "*",
    "grunt-contrib-uglify": "*",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-webpack": "*",
    "jest-cli": "^0.7.1",
    "svg.js": "2.2.5",
    "webpack": "^1.12.2"
  },
  "scripts": {
    "test": "jest",
    "prepublish": "grunt"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/babel-jest-processor.js",
    "preprocessCachingDisabled": true,
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "unmockedModulePathPatterns": [
      "models",
      "util",
      "clone"
    ]
  }
}
